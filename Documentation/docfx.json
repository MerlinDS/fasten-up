{
  "metadata": [
    {
      "src": [
        {
          "src": "../",
          "files": [ "FastenUp.Runtime.csproj" ]
        }
      ],
      "filter": "filterConfig.yml",
      "dest": "api"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "**/*.{md,yml}"
        ],
        "exclude": [
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "output": "_site",
    "template": [
      "default",
      "modern"
    ],
    "globalMetadata": {
      "_appName": "FastenUp",
      "_appTitle": "FastenUp",
      "_enableSearch": true,
      "pdf": false
    },
    "xref": [
      "https://normanderwan.github.io/UnityXrefMaps/xrefmap.yml"
    ],
    "xrefService": [
      "https://xref.docs.microsoft.com/query?uid={uid}"
    ]
  }
}